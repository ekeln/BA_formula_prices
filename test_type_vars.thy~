theory test_type_vars
imports Main
begin
datatype rand = p | q | r

fun krz:: "'a \<Rightarrow> 'a \<Rightarrow> bool" where
"krz x y = (if x = y then True else False)"

fun frst:: "'a \<Rightarrow> 'a \<Rightarrow> 'a" where
"frst x y = x"


lemma bool_test: "krz p (frst q p) = False"
  by simp

fun app:: "'a list \<Rightarrow> 'a list \<Rightarrow> 'a list" where
"app Nil ys = ys"
| "app (x#xs) ys = x # (app xs ys)"


lemma test: "app [p, p, p] [q] = [p, p, p, q]"
  apply simp
  done
end